<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="reg">
   <insert id="add" parameterType="java.util.Map">
      INSERT INTO REGIST_T(s_id,s_pw,s_name,s_gender,status,b_idx)
      VAlUES ( Regist_t_seq.NEXTVAL,#{s_pw},#{s_name},#{s_gender},0,#{b_idx})
   </insert>	
</mapper>